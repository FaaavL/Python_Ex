def formata(municipio, turno2, cands):
  nome1 = cands[0]['nome']
  nome2 = cands[1]['nome']
  votos1 = cands[0]['votos']
  votos2 = cands[1]['votos']
  partido1 = cands[0]['partido']
  partido2 = cands[1]['partido']
  cadeiras = cands[0]['maxcad']
  numero = cands[0]['numero']
  nfoi = cands[0]['nfoi']
  foi = cands[0]['foi']
  branfoi = cands[0]['branfoi']
  if turno2 == 1:
    return f'{nome1} do {partido1} e {nome2} do {partido2} disputam 2º turno em {municipio}\n' + f'{nome1} teve {votos1}% dos votos dados a todos os candidatos(as) e {nome2}, {votos2}%. Os dois vão decidir a eleição no dia 29 de novembro. O(s) partido(s) com maioria de cadeiras em {municipio} é o {cadeiras} com {numero} cadeiras. Ademais, houve {nfoi}% de abstinências e do total dos {foi}% que compareceram, {branfoi}% votaram em branco ou anularam o voto.'
  elif turno2 == 2:
    return f'{nome1}, do {partido1}, é eleito prefeito(a) de {municipio}\n' + f'Ele teve {votos1}% dos votos em uma eleição de chapa única. O(s) partido(s) com maioria de cadeiras no {municipio} é o {cadeiras} com {numero} cadeiras. Ademais, houve {nfoi}% de abstinências e do total dos {foi}% que compareceram, {branfoi}% votaram em branco ou anularam o voto.'
  else:
    partido1 = cands[0]['partido']
    return f'{nome1}, do {partido1}, é eleito prefeito(a) de {municipio}\n' + f'Ele teve {votos1}% dos votos dados a todos os candidatos(as) e derrotou {nome2}, que ficou em segundo lugar com {votos2}%. O(s) partido(s) com maioria de cadeiras no {municipio} é o {cadeiras} com {numero} cadeiras. Ademais, houve {nfoi}% de abstinências e do total dos {foi}% que compareceram, {branfoi}% votaram em branco ou anularam o voto.'


for municipio, dados in municipios.items():

  turno2 = dados['turno2']
  print('-----')
  print(formata(municipio, turno2, dados['candidatos']))
